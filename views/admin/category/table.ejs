<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Nama Kategori</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% category.forEach((category, i) => { i++ %>
    <tr>
      <td><%= i %></td>
      <td><%= category.name %></td>
      <td>
        <form action="">
          <a href="" class="btn btn-warning">
            <i class="fas fa-pencil-alt"></i>
            Ubah
          </a>
          <button class="btn btn-danger">
            <i class="fas fa-trash-alt"></i>
            Hapus
          </button>
        </form>
      </td>
    </tr>
    <% }) %>

  </tbody>
</table>